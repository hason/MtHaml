--JADE--
- set id = 5
- function answer() { return 42; }
a(href='/user/' + id, class='button')
a(href  =  '/user/' + id, class  =  'button')
meta(key='answer', value=answer())
a(class = ['class1', 'class2'])
a.tag-class(class = ['class1', 'class2'])

a(href='/user/' + id class='button')
a(href  =  '/user/' + id class  =  'button')
meta(key='answer' value=answer())
a(class = ['class1', 'class2'])
a.tag-class(class = ['class1', 'class2'])

div(id=id)&attributes({foo: 'bar'})
- set bar = null
div(foo=null bar=bar)&attributes({baz: 'baz'})

