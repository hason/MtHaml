--JADE--

- set items = [1,2,3]

ul
  - items.forEach(function(item){
    li= item
  - })

- set items = [1,2,3]

ul
  for item, i in items
    li(class='item-#{i}')= item

ul
  each item, i in items
    li= item

ul
  each $item in items
    li= $item

- set nums = [1, 2, 3]
- set letters = ['a', 'b', 'c']

ul
  for l in letters
    for n in nums
      li #{n}: #{l}

- set count = 1
- set counter = function() { return [count++, count++, count++] }
ul
  for n in counter()
    li #{n}
--EXPECT--
<ul>
