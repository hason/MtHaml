--JADE--

extend auxiliary/layout

mixin article(title)
  if title
    h1= title
  block

block content
  +article("The meaning of life")
    p Foo bar baz!
--EXPECT--
<html>
  <head>
    <title>My Application</title>
  </head>
  <body>
    <h1>The meaning of life</h1>
    <p>Foo bar baz!</p>
  </body>
</html>